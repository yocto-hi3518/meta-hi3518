+		pstAeSnsGainInfo->u32SnsTimes = analog_gain_table[63];
+		pstAeSnsGainInfo->u32GainDb = 63;
+		return ;
+	}
+
+	for (i = 1; i < 64; i++)
+	{
+		if (u32InTimes < analog_gain_table[i])
+		{
+			pstAeSnsGainInfo->u32SnsTimes = analog_gain_table[i - 1];
+			pstAeSnsGainInfo->u32GainDb = i - 1;
