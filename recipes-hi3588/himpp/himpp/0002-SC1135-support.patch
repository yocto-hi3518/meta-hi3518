+static void delay_ms(int ms) {
+	usleep(ms*1000);
+}
+
+void sensor_prog(int* rom)
+{
+	int i = 0;
+	while (1) {
+		int lookup = rom[i++];
+		int addr = (lookup >> 16) & 0xFFFF;
+		int data = lookup & 0xFFFF;
+		if (addr == 0xFFFE) {
+			delay_ms(data);
+		} else if (addr == 0xFFFF) {
+			return;
+		} else {
+			sensor_write_register(addr, data);
+		}
+	}
+}
